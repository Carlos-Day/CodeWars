function gimme (triplet) {
  //Param: given an array of three numbers. They will always be numbers, no empty arrays. 
  //Returns: return one number. Return the original index of the num which is the middle number 
  
  //create a copy of the array
  let copy = triplet.slice()
  //sort the copied array numerically
  copy.sort((a, b) => a-b)
  //Get the value of the middle number and find where that number is in the original array
  return triplet.indexOf(copy[1])

}